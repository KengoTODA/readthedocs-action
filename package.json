{
  "name": "rtd-bot",
  "version": "0.7.3",
  "description": "An unofficial GitHub Probot that helps reveiwing on Read the Docs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "pretest": "tslint -p tsconfig.json",
    "test": "mocha",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "probot",
    "readthedocs",
    "rtd"
  ],
  "author": "Kengo TODA",
  "license": "Apache-2.0",
  "dependencies": {
    "@octokit/rest": "^16.17.1",
    "@types/bunyan": "^1.8.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.16.0",
    "@types/node-fetch": "^2.1.2",
    "@types/promise-retry": "^1.1.2",
    "@types/puppeteer": "^1.6.2",
    "dotenv": "^8.0.0",
    "node-fetch": "^2.2.0",
    "probot": "^9.0.0",
    "promise-retry": "^1.1.1",
    "puppeteer": "^2.0.0",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": "^12.12",
    "yarn": "^1.17"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^6.0.0",
    "commitizen": "^4.0.0",
    "cz-conventional-changelog": "^3.0.1",
    "mocha": "^7.0.0",
    "mocha-assume": "^1.0.0",
    "semantic-release": "^16.0.0",
    "travis-deploy-once": "^5.0.7",
    "tslint": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KengoTODA/rtd-bot.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "heroku-run-build-script": true
}
